@*Back up DISPLAY TOPIC AND EXAMPLE
    @{int i = 1;}
    <div class="accordion" id="accordionExample">
        <div class="card">
            @foreach (var topic in Model.TopicItems)
            {
                <div class="card-header" id="headingOne">
                    <h5 class="mb-0">
                        <button class="btn btn-link" type="button" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne" style="color: black;">
                            @Html.DisplayFor(x => topic.Name)
                        </button>
                    </h5>
                </div>

                <div id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#accordionExample">
                    <div class="card-body">
                        <ul>
                            @foreach (var exampleName in topic.Examples)
                            {
                                <li>
                                    <a href=@Url.Action("show", "Example", new { exampleID = exampleName.Id}, null)>
                                        @Html.DisplayFor(y => exampleName.Name)
                                    </a>
                                </li>
                            }
                        </ul>
                    </div>
                </div>

                <div style="width: 90%" id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#accordionExample">
                    <p class="float-right">
                        <a href=@Url.Action("Create", "Example", new { topicID = topic.Id}, null)>
                            <img class="add-button-hover" src="~/Images/Add_button.jpeg" alt="Add Example" title="Add Example" height="40" width="40">
                        </a>
                    </p>
                </div>
            }
        </div>
    </div>*@